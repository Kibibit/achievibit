{
  "name": "achievibit",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest start",
    "start:dev-watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch --coverage",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-barrels": "barrelsby --delete -d ./src -l below -q --exclude spec.ts",
    "coveralls": "cat ./test-results/coverage/lcov.info | coveralls",
    "commit": "npx git-cz",
    "deploy-tests-report": "./deploy-test-results.js"
  },
  "dependencies": {
    "@hapi/joi": "^16.1.8",
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^4.0.9",
    "@types/find-root": "^1.1.1",
    "@types/nconf": "^0.10.0",
    "@types/nunjucks": "^3.1.3",
    "chance": "^1.1.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "dotenv": "^8.2.0",
    "find-root": "^1.1.0",
    "fs-extra": "^8.1.0",
    "gh-badges": "^2.2.1",
    "hbs": "^4.0.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongodb": "^3.3.5",
    "mongodb-memory-server": "^6.0.2",
    "mongoose": "^5.8.0",
    "nconf": "^0.10.0",
    "nunjucks": "^3.2.0",
    "randexp": "^0.5.3",
    "reflect-metadata": "^0.1.13",
    "require-all": "^3.0.0",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "smee-client": "^1.1.0",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.7.1",
    "@nestjs/typeorm": "^6.2.0",
    "@octokit/rest": "^16.35.2",
    "@types/chance": "^1.0.7",
    "@types/depcheck": "^0.9.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.1",
    "@types/hapi__joi": "^16.0.4",
    "@types/jest": "^24.0.18",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.149",
    "@types/moment": "^2.13.0",
    "@types/mongoose": "^5.5.32",
    "@types/node": "^12.7.5",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^3.4.6",
    "barrelsby": "^2.2.0",
    "commitizen": "^4.0.3",
    "coveralls": "^3.0.9",
    "cz-conventional-changelog": "^3.0.2",
    "depcheck": "^0.9.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-github-reporter": "^0.3.4",
    "jest-stare": "^1.25.0",
    "jsdom": "^15.2.1",
    "now": "^16.7.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "jest": {
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "html"
    ],
    "verbose": true,
    "reporters": [
      "default",
      "jest-stare",
      "jest-github-reporter"
    ],
    "testResultsProcessor": "./node_modules/jest-stare",
    "watchPathIgnorePatterns": [
      ".*test-results.*\\.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "src/app.service.ts",
      "!src/**/index.ts",
      "!src/**/*.module.ts",
      "!src/main.ts",
      "!src/*.mock-generator.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./test-results/coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@kb-modules$": "<rootDir>/src/modules",
      "^@kb-models$": "<rootDir>/src/models",
      "^@kb-config$": "<rootDir>/src/config/index",
      "^@kb-errors$": "<rootDir>/src/errors/index",
      "^@kb-dev-tools$": "<rootDir>/src/dev-tools/index",
      "^@kb-app$": "<rootDir>/src/app/index",
      "^@kb-engines$": "<rootDir>/src/engines/index"
    }
  },
  "jest-stare": {
    "resultDir": "test-results/",
    "coverageLink": "./coverage/index.html"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && (npm run commit -- --hook) || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS || (git config commit.template .commit-template && false)"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
