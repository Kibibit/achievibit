{
  "name": "@kibibit/kb-server-client-template",
  "version": "0.0.0-development",
  "description": "<DESCRIPTION>",
  "main": "dist/server/main.js",
  "scripts": {
    "prune-branches": "node ./tools/scripts/prune-untrackted-branches.js",
    "contributors:all": "cross-env HUSKY_SKIP_HOOKS=1 node ./tools/get-all-contributors.js",
    "contributors:add": "cross-env HUSKY_SKIP_HOOKS=1 all-contributors add",
    "contributors:generate": "cross-env HUSKY_SKIP_HOOKS=1 all-contributors generate",
    "install": "npm run install:server && npm run install:client",
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "build": "npm run build:server && npm run build:client",
    "build:client": "cd ./client && npm run build",
    "build:server": "cd ./server && npm run build",
    "start:client": "cd ./client && npm run start",
    "start:server": "cd ./server && npm run start:dev",
    "init": "node ./tools/replace-template-string.js",
    "generate-barrels": "barrelsby --delete -d ./src -l below -q",
    "semantic-release:setup": "semantic-release-cli setup",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:server-unit": "cd server && npm run test:cov",
    "test:client-unit": "cd client && npm run test:cov",
    "test:api": "cd server && npm run test:e2e",
    "test:e2e": "cd client && npm run e2e",
    "semantic-release": "semantic-release"
  },
  "author": "thatkookooguy <neilkalman@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/cron": "^1.7.2",
    "@types/find-root": "^1.1.2",
    "@types/mongoose": "^5.10.3",
    "@types/socket.io": "^2.1.13",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "all-contributors-cli": "^6.20.0",
    "cli-table": "^0.3.6",
    "commitizen": "^4.2.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^7.19.0",
    "foswig": "^3.0.1",
    "github-username": "^6.0.0",
    "gitlog": "^4.0.4",
    "husky": "^4.3.8",
    "inquirer": "^8.0.0",
    "jest-stare": "^2.2.1",
    "karma-htmlfile-reporter": "^0.3.8",
    "ncp": "^2.0.0",
    "package-name-regex": "^1.0.11",
    "replace-in-file": "^6.2.0",
    "semantic-release": "^17.3.8",
    "semantic-release-cli": "^5.4.3",
    "sentencer": "^0.2.1",
    "shelljs": "^0.8.4",
    "simple-git": "^2.37.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kibibit/kb-server-client-template.git"
  },
  "bugs": {
    "url": "https://github.com/Kibibit/kb-server-client-template/issues"
  },
  "dependencies": {
    "@kibibit/consologo": "^1.2.0",
    "@kibibit/kb-error": "^1.0.3",
    "@nestjs/common": "^7.6.11",
    "@nestjs/core": "^7.6.11",
    "@nestjs/mongoose": "^7.0.4",
    "@nestjs/platform-express": "^7.6.11",
    "@nestjs/platform-socket.io": "^7.6.15",
    "@nestjs/platform-ws": "^7.6.15",
    "@nestjs/schedule": "^0.4.3",
    "@nestjs/swagger": "^4.7.12",
    "@nestjs/websockets": "^7.6.15",
    "@typegoose/typegoose": "^7.6.0",
    "axios": "^0.21.1",
    "class-transformer": "^0.3.2",
    "class-validator": "^0.13.1",
    "find-root": "^1.1.0",
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.19",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
